{
    "name": "copy-copy-paste",
    "displayName": "Copy Copy Paste",
    "description": "Clipboard history that stores what you copy and hands it back when you want it.",
    "version": "0.0.1",
    "publisher": "rockingskier",
    "license": "MIT",
    "engines": {
        "vscode": "^1.14.0"
    },
    "preview": true,
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "copy-copy-paste.copy",
                "title": "Copy Copy Paste: Copy",
                "when": "editorTextFocus"
            },
            {
                "command": "copy-copy-paste.cut",
                "title": "Copy Copy Paste: Cut",
                "when": "editorTextFocus"
            },
            {
                "command": "copy-copy-paste.history",
                "title": "Copy Copy Paste: History",
                "when": "editorTextFocus"
            },
            {
                "command": "copy-copy-paste.clear",
                "title": "Copy Copy Paste: Clear history"
            }
        ],
        "keybindings": [
            {
                "command": "copy-copy-paste.copy",
                "key": "ctrl+c",
                "mac": "cmd+c",
                "when": "editorTextFocus"
            },
            {
                "command": "copy-copy-paste.cut",
                "key": "ctrl+x",
                "mac": "cmd+x",
                "when": "editorTextFocus"
            },
            {
                "command": "copy-copy-paste.history",
                "key": "shift+ctrl+v",
                "mac": "shift+cmd+v",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {}
}
